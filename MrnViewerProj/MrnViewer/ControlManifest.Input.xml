<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <control namespace="MrnViewer" constructor="MrnViewer" version="0.0.1" display-name-key="MrnViewer" description-key="Transform a textbox control into a tabbed view of Patient data such as Demographics, Eligibility, Benefits." control-type="standard">
	<property name="mrnPlusRegion" display-name-key="MrnPlusRegion" description-key="Enter MRN-Region (MRN seperated by hyphen)" of-type="SingleLine.Text" usage="bound" required="true" />
    <resources>
      <code path="index.ts" order="1"/>
      <css path="css/mrnViewer.css" order="1" />
      <css path="css/bootstrap.css" order="2" />
    </resources>
    <feature-usage>
      <uses-feature name="WebAPI" required="true" />
    </feature-usage>
  </control>
</manifest>